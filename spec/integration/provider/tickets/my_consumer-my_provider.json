{
  "consumer": {
    "name": "my_consumer",
    "label": "My Consumer"
  },
  "provider": {
    "name": "my_provider",
    "label": "My Provider"
  },
  "interactions": [
    {
      "request": {
        "type": "and_expression",
        "values": {
          "path": {
            "type": "string",
            "value": "/weather"
          },
          "method": {
            "type": "string",
            "value": "get"
          }
        }
      },
      "response": {
        "type": "and_expression",
        "values": {
          "status": {
            "type": "number",
            "value": 200
          },
          "body": {
            "type": "regexp",
            "value": "(?-mix:cold day.*5 degrees)"
          }
        }
      },
      "time": "2013-11-05T08:22:34Z",
      "label": "greeting",
      "fixtures": [
        {
          "name": "weather is cold",
          "arguments": {
            "temperature": 5
          }
        }
      ]
    }
  ],
  "version": "0.0.0"
}